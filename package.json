{
  "name": "@antdvr/eslint-config-3.x",
  "description": "ESlint Config Plugin of Antdvr",
  "homepage": "https://github.com/Antdvr/eslint-config-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT",
  "version": "4.1.0",
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Antdvr/eslint-config-3.x.git"
  },
  "bugs": {
    "url": "https://github.com/Antdvr/eslint-config-3.x/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "eslint": "npx eslint --fix --quiet src/**"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.12.3"
}
